  "version": "0.2.0",
  "configurations": [
    {
      "name": "RunServiceOnce urldownloader",
      "type": "python",
      "request": "launch",
      "module": "assemblyline_v4_service.dev.run_service_once",
      "cwd": "${workspaceFolder}/al-services/assemblyline-service-urldownloader/",
      "args": ["-d", "urldownloader.urldownloader.URLDownloader", "${file}"],
      "justMyCode": false
    },
    {
      "name": "[Service] urldownloader - Privileged",
      "type": "python",
      "request": "launch",
      "module": "assemblyline_v4_service.run_privileged_service",
      "env": {
        "SERVICE_MANIFEST_PATH": "service_manifest.yml",
        "PRIVILEGED": "true",
        "SERVICE_PATH": "urldownloader.urldownloader.URLDownloader",
        "TASKING_DIR": "/tmp/urldownloader"
      },
      "console": "internalConsole",
      "cwd": "${workspaceFolder}/al-services/assemblyline-service-urldownloader/",
      "justMyCode": false
    }
  ]
}
